set_digital_out(8,False)
timeout = 0
while get_digital_in(9) == True:
    timeout = timeout +1
    sleep(0.008)
    if timeout > 500:
        break
    end
end

timeout = 0
while get_digital_in(9) == False:
    timeout = timeout +1
    sleep(0.008)
    if timeout > 500:
        popup("RG2 is not responding", title="Error",  warning = False, error=True, blocking=True)
        break
    end
end